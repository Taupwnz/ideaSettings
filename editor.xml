<application>
  <component name="CodeInsightSettings">
    <option name="OPTIMIZE_IMPORTS_ON_THE_FLY" value="true" />
    <EXCLUDED_PACKAGE NAME="jdk.nashorn.internal" />
    <EXCLUDED_PACKAGE NAME="org.codehaus.jackson" />
    <EXCLUDED_PACKAGE NAME="org.gradle.internal.impldep" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="JAVA" value="true" />
        <entry key="XML" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="SHOW_BREADCRUMBS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="question" pattern="\btodo\b \bArtÑ‘m\b \bBasov\b.*" />
    <filter name="My TODOs">
      <pattern index="2" />
    </filter>
  </component>
</application>